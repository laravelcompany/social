{
    "name": "cornatul/social",
    "description": "A set of tools to share content on social media",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Stefan I",
            "email": "Stefan@Cornatul.com",
            "homepage": "https://cornatul.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cornatul\\Social\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cornatul\\Social\\Tests\\": "tests/",
            "Cornatul\\Social\\Factories\\": "database/factories/"
        }
    },
    "require": {
        "php": "^8.1||^8.0||^7.4",
        "atymic/twitter": "3.*",
        "google/apiclient": "^2.12.1",
        "guzzlehttp/guzzle": ">=7.5.0",
        "illuminate/support": ">=9.0.0",
        "jonathantorres/medium-sdk": "dev-master",
        "league/oauth1-client": "^1.10",
        "league/oauth2-facebook": "dev-main",
        "league/oauth2-github": "dev-main",
        "league/oauth2-google": "dev-main",
        "league/oauth2-instagram": "dev-main",
        "league/oauth2-linkedin": "^5.1",
        "smolblog/oauth2-twitter": "dev-main",
        "spatie/laravel-data": ">=3.0.0",
        "tumblr/tumblr": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cornatul\\News\\NewsServiceProvider"
            ]
        }
    }
}
